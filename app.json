{
  "expo": {
    "name": "Jobs",
    "slug": "rn-jobs",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devsingh.rnjobs"
    },
    "android": {
      "package": "com.devsingh.rnjobs"
    },
    "description": "React Native Expo project for searching jobs in some areas via github jobs api",
    "githubUrl": "https://github.com/Devendra616/rnjobs"
  }
}
